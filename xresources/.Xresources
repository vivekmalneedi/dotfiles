urxvt.termName: rxvt-unicode
urxvt.scrollBar: false
!urxvt.background: black
!urxvt.foreground: gray
urxvt.transparent: false
urxvt.font: xft:DroidSansMono Nerd Font Mono:pixelsize=32
urxvt.boldFont: xft:DroidSansMono Nerd Font Mono:pixelsize=32:weight=bold
!urxvt.letterSpace: -1
urxvt.saveLines: 1000000
URxvt.internalBorder:  0

URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,matcher,clipboard
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.keysym.M-u: perl:matcher:selec

*.foreground:   #ffeefc
*.background:   #141021
*.cursorColor:  #c16772

! black
URxvt*.color0:       #4f4858
URxvt*.color8:       #545d67

! red
URxvt*.color1:       #c05c47
URxvt*.color9:       #c16772

! green
URxvt*.color2:       #428e8e
URxvt*.color10:      #afda6e

! yellow
URxvt*.color3:       #e4b226
URxvt*.color11:      #ebcf1a

! blue
URxvt*.color4:       #4457a3
URxvt*.color12:      #4880a5

! magenta
URxvt*.color5:       #903a62
URxvt*.color13:      #9568a1

! cyan
URxvt*.color6:       #7aa1dc
URxvt*.color14:      #abbcda

! white
URxvt*.color7:       #c4b7c5
URxvt*.color15:      #ffeefc

rofi.color-enabled: true
rofi.color-window: #141021, #c16772, #9568a1
